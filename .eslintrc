{
    "rules": {
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            0,
            "windows"
        ],
        "semi": [
            2,
            "never"
        ],
        "no-unused-vars": [
            1
        ],
        "no-console":"off"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
